.title {
    font-size: 18px;
    text-align: center;
    margin-top: 18rpx;
}

.list {
    margin-top: 60rpx;
}

.btn {
    padding: 24rpx 0 24rpx 24rpx;
    border-bottom: 1px solid #E0E0E0;
}

.navigator-hover {
    color: blue;
}

.other-navigator-hover {
    color: red;
}


.remark {
    position: absolute;
    z-index: 999;
    bottom: 0;
    padding: 0 24rpx 24rpx 24rpx;
}


.order {
    padding-top: 15px;
    padding-left: 15px;
    padding-bottom: 15px;
    font-size: 15px;
    height: 80rpx;
    width: 100%;
    box-sizing: border-box;
    overflow: hidden;
    line-height: 30rpx;
    background: #87CEEB;
    font-size: 20px;
    white-space: nowrap;
    left: 0;
    z-index: 0;
  }
  .tab-h {
    padding-left: 15px;
    height: 80rpx;
    width: 100%;
    font-size: 16px;
  }
  .tab-h1 {
    padding-left: 15px;
    padding-top: 50px;
    height: 80rpx;
    width: 100%;
    font-size: 16px;
  }
  .tab-h2 {
    padding-left: 15px;
    padding-top: 50px;
    height: 80rpx;
    width: 100%;
    font-size: 16px;
  }
  .tag {
    color: #999;
  }
  .hr {
    width: 100%;
    height: 15px;
    background-color: #f5f5f5;
  }
  .line {
    height: 1px;
    width: 100%;
    background-color: #666666;
    opacity: 0.2;
  }
  .nav {
    display: flex;
    flex-direction: row;
    padding-top: 10px;
    padding-bottom: 10px;
  }
  .nav-item {
    width: 25%;
    font-size: 13px;
    text-align: center;
    margin: 0 auto;
  }
  
  .item {
    display: flex;
    flex-direction: row;
  }
  .detail2 {
    font-size: 15px;
    position: absolute;
    right: 10px;
    margin-top: 15px;
    color: #888888;
  }